        <item name="colorPrimary">#5492FD</item>
        <item name="colorPrimaryVariant">#5492FD</item>
        <item name="colorOnPrimary">#5492FD</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
